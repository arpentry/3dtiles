{
  "name": "3dtiles-demo",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel dev",
    "build": "pnpm --parallel build",
    "deploy": "pnpm --parallel deploy",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:api": "pnpm --filter 3dtiles-api lint",
    "lint:web": "pnpm --filter 3dtiles-web lint",
    "lint:api:fix": "pnpm --filter 3dtiles-api lint:fix",
    "lint:web:fix": "pnpm --filter 3dtiles-web lint:fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:api": "pnpm --filter 3dtiles-api test",
    "test:web": "pnpm --filter 3dtiles-web test",
    "deploy:api": "pnpm --filter 3dtiles-api run deploy",
    "build:web": "pnpm --filter 3dtiles-web run build",
    "deploy:web": "pnpm build:web && pnpm --filter 3dtiles-web run deploy",
    "deploy:all": "pnpm --parallel run deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "vitest": "^3.2.4"
  }
}
