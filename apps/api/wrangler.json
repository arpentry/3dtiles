{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "3dtiles-api",
  "main": "src/index.ts",
  "compatibility_date": "2025-02-12",
  "assets": {
    "directory": "public"
  },
  "observability": {
    "logs": {
      "enabled": true
    }
  },
  "env": {
    "R2_PUBLIC_ARPENTRY_ENDPOINT": "http://localhost:8787"
  },
  "limits": {
    "cpu_ms": 600000
  }
}
